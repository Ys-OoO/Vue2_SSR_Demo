{
  "name": "vue2_ssr_demo",
  "version": "1.0.0",
  "description": "Ys-OoO's Personal Site",
  "scripts": {
    "dev": "concurrently \"npm run build:dev\" \"nodemon server\"",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "start": "cross-env NODE_ENV=production node server",
    "build:dev": "rimraf dist && concurrently \"npm run build:client:dev\" \"npm run build:server:dev\"",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.js",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.js",
    "build:client:dev": "cross-env NODE_ENV=development webpack --config build/webpack.client.js --watch",
    "build:server:dev": "cross-env NODE_ENV=development webpack --config build/webpack.server.js --watch"
  },
  "author": "Ys_OoO <ys0514@yeah.net>",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^7.0.3",
    "express": "^4.21.1",
    "vue": "^2.7.16",
    "vue-router": "^3.6.5",
    "vue-server-renderer": "^2.7.16",
    "vuex": "^3.6.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.0.1",
    "css-loader": "^7.1.2",
    "nodemon": "^3.1.7",
    "rimraf": "^6.0.1",
    "sass": "^1.79.4",
    "sass-loader": "^16.0.2",
    "ts-loader": "^9.5.1",
    "vue-loader": "^15.10.0",
    "vue-style-loader": "^4.1.3",
    "webpack-dev-server": "^5.1.0",
    "webpack-merge": "^6.0.1"
  }
}
